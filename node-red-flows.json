    {
        "id": "parse_status",
        "type": "function",
        "z": "flow1",
        "name": "Parse Lock Data",
        "func": "const payload = msg.payload;\nconst battery = payload.batteryLevel || 0;\nconst signal = payload.signalStrength || 0;\nconst lockId = payload.lockId || 'Unknown';\n\nnode.warn('Received: battery=' + battery + ', signal=' + signal + ', lockId=' + lockId);\n\n// Create message with lockId in topic for routing\nmsg.payload = battery;\nmsg.topic = lockId;\nmsg.signal = signal;\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 370,
        "y": 100,
        "wires": [["route_by_lock"]]
    },
    {
        "id": "route_by_lock",
        "type": "switch",
        "z": "flow1",
        "name": "Route by Lock ID",
        "property": "topic",
        "propertyType": "msg",
        "rules": [
            {"t": "eq", "v": "lock_gateway_001_1", "vt": "str"},
            {"t": "eq", "v": "lock_gateway_001_2", "vt": "str"},
            {"t": "eq", "v": "lock_gateway_001_3", "vt": "str"},
            {"t": "eq", "v": "lock_gateway_002_1", "vt": "str"},
            {"t": "eq", "v": "lock_gateway_002_2", "vt": "str"},
            {"t": "eq", "v": "lock_gateway_002_3", "vt": "str"}
        ],
        "checkall": "false",
        "repair": false,
        "outputs": 6,
        "x": 560,
        "y": 100,
        "wires": [
            ["battery_gw1_lock1", "signal_gw1_lock1"],
            ["battery_gw1_lock2", "signal_gw1_lock2"],
            ["battery_gw1_lock3", "signal_gw1_lock3"],
            ["battery_gw2_lock1", "signal_gw2_lock1"],
            ["battery_gw2_lock2", "signal_gw2_lock2"],
