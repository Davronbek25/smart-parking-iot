    {
        "id": "test_inject",
        "type": "inject",
        "z": "flow1",
        "name": "Test Data",
        "props": [
            {
                "p": "payload",
                "v": "{\"batteryLevel\": 85, \"signalStrength\": 90, \"lockId\": \"test_lock\"}",
                "vt": "json"
            }
        ],
        "repeat": "",
        "crontab": "",
        "once": false,
        "onceDelay": 0.1,
        "topic": "",
        "x": 100,
        "y": 200,
        "wires": [["parse_status"]]
    },
    {
        "id": "mqtt_in_1",
        "type": "mqtt in",
        "z": "flow1",
        "name": "Lock Status Updates",
        "topic": "/+/up_link",
        "qos": "2",
        "datatype": "json",
        "broker": "mqtt_broker",
        "nl": false,
        "rap": true,
        "rh": 0,
        "inputs": 0,
        "x": 150,
        "y": 100,
        "wires": [["debug_mqtt", "parse_status"]]
