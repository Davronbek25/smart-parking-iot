    {
        "id": "flow2",
        "type": "tab",
        "label": "Alerting System",
        "disabled": false,
        "info": "Monitors for low battery, weak signal, and status changes"
    },
    {
        "id": "mqtt_in_2",
        "type": "mqtt in",
        "z": "flow2",
        "name": "Monitor Status",
        "topic": "/+/up_link",
        "qos": "2",
        "datatype": "json",
        "broker": "mqtt_broker",
        "nl": false,
        "rap": true,
        "rh": 0,
        "inputs": 0,
        "x": 140,
        "y": 100,
        "wires": [["switch_alerts"]]
    },
    {
        "id": "switch_alerts",
        "type": "function",
        "z": "flow2",
        "name": "Check Thresholds",
        "func": "const payload = msg.payload;\nconst battery = payload.batteryLevel || 0;\nconst signal = payload.signalStrength || 0;\nconst status = payload.status || 'unknown';\n\nlet output = [null, null, null];\n\n// Check battery threshold\nif (battery < 20) {\n    output[0] = msg;\n}\n\n// Check signal threshold\nif (signal < 50) {\n    output[1] = msg;\n}\n\n// Check status change to occupied\nif (status === 'occupied') {\n    output[2] = msg;\n}\n\nreturn output;",
        "outputs": 3,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 340,
        "y": 100,
        "wires": [["format_battery_alert"], ["format_signal_alert"], ["format_status_alert"]]
    },
    {
        "id": "format_battery_alert",
        "type": "function",
        "z": "flow2",
        "name": "Format Battery Alert",
        "func": "msg.payload = {\n    alert: 'LOW_BATTERY',\n    lockId: msg.payload.lockId,\n    batteryLevel: msg.payload.batteryLevel,\n    message: `LOW BATTERY: ${msg.payload.lockId} at ${msg.payload.batteryLevel}%`,\n    timestamp: new Date().toISOString()\n};\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 580,
        "y": 60,
        "wires": [["debug_alert", "log_alert"]]
    },
    {
        "id": "format_signal_alert",
        "type": "function",
        "z": "flow2",
        "name": "Format Signal Alert",
        "func": "msg.payload = {\n    alert: 'WEAK_SIGNAL',\n    lockId: msg.payload.lockId,\n    signalStrength: msg.payload.signalStrength,\n    message: `WEAK SIGNAL: ${msg.payload.lockId} at ${msg.payload.signalStrength}%`,\n    timestamp: new Date().toISOString()\n};\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 580,
        "y": 100,
        "wires": [["debug_alert", "log_alert"]]
    },
    {
        "id": "format_status_alert",
        "type": "function",
        "z": "flow2",
        "name": "Format Status Alert",
        "func": "msg.payload = {\n    alert: 'STATUS_CHANGE',\n    lockId: msg.payload.lockId,\n    status: msg.payload.status,\n    message: `STATUS CHANGE: ${msg.payload.lockId} is now ${msg.payload.status}`,\n    timestamp: new Date().toISOString()\n};\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 580,
        "y": 140,
        "wires": [["debug_alert", "log_alert"]]
    },
