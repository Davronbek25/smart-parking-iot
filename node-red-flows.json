    {
        "id": "format_log",
        "type": "function",
        "z": "flow3",
        "name": "Format Log Entry",
        "func": "const timestamp = new Date().toISOString();\nconst topic = msg.topic;\nlet payload = msg.payload;\n\nif (typeof payload === 'object') {\n    payload = JSON.stringify(payload);\n}\n\nmsg.payload = {\n    timestamp: timestamp,\n    topic: topic,\n    message: payload\n};\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 330,
        "y": 100,
        "wires": [["csv_convert", "debug_log"]]
    },
    {
        "id": "csv_convert",
        "type": "csv",
        "z": "flow3",
        "name": "Convert to CSV",
        "sep": ",",
        "hdrin": false,
        "hdrout": "all",
        "multi": "one",
        "ret": "\\n",
        "temp": "timestamp,topic,message",
        "skip": "0",
        "strings": true,
        "include_empty_strings": false,
        "include_null_values": false,
        "x": 540,
        "y": 100,
        "wires": [["write_csv"]]
