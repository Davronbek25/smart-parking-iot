{
  "name": "smart-parking-iot",
  "version": "1.0.0",
  "description": "Smart Parking Reservation IoT System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "broker": "node mqtt-broker.js",
    "start-gateway": "node simulators/gateway.js",
    "start-system": "concurrently \"npm run broker\" \"npm start\" \"npm run start-gateway gateway_001\" \"npm run start-gateway gateway_002\"",
    "node-red": "node-red",
    "start-system-full": "concurrently \"npm run broker\" \"npm start\" \"npm run start-gateway gateway_001\" \"npm run start-gateway gateway_002\" \"npm run node-red\""
  },
  "keywords": ["iot", "parking", "mqtt", "nodejs"],
  "author": "Davronbek Orifjonov",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mqtt": "^5.3.4",
    "socket.io": "^4.7.4",
    "cors": "^2.8.5",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "aedes": "^0.50.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}